<!DOCTYPE html>
<html>
    <head>
            <link rel="stylesheet" type="text/css" href="CSS/custom.css"/>
    </head>
<body onload="startGame()">

    <header><h1>Welcome to Minesweeper</h1></header>
    <script type="text/javascript" src="JS/handler.js"></script>
    <div id="time"></div><div id="score" style="display:block"></div>
    <div class='wrapper'>
    <% for(var i=1;i<=8;i++){
        for(var j=1;j<=8;j++){
        %>
        <div class='box' id='button<%=i%><%=j%>'>
            <div id='res<%=i%><%=j%>' style="height:100%;width:100%;display:block;" onclick='changeState(this)'>
            </div>
        </div>
      <%}%>
    <%} %>
</div>

</body>
<script>
        newGame()
</script>
</html>